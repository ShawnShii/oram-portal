
<div id="events">
  <h1>Admins</h1>
  
  <!-- Responsive table starts here -->
  <!-- For correct display on small screens you must add 'data-title' to each 'td' in your table -->
  <div class="table-responsive-vertical shadow-z-1">
  <!-- Table starts here --> 
  <table id="table" class="table table-striped table-mc-light-blue">
      <thead>
        <tr>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Profile</th>
        </tr>
      </thead>
      <tbody>
      	<% @admins.each do |admin| %>
			<tr>
				<td><%= if !admin.nil?  then admin.first_name end %></td>
				<td><%= if !admin.nil?  then admin.last_name end%></td>
				<td><%= link_to "View Profile", admin_path(admin.id) %></td>
			</tr>
		<% end %>
      </tbody>
    </table>
  </div>
</div>
