<div class="panel panel-default panel-form">
  <div class="panel-heading">
  	<h4> Change Your Profile </h4>
  </div>
  <div class="panel-body">
   
   <h2 class="header-link content-link"><%= button_to "Edit Profile", admin_settings_edit_path(current_admin.id), {:method => :get, class: "hvr-underline-from-left btn btn-lg btn-default"}%> </h2>
   <br></br>
   
   <h2 class="header-link content-link"><%= button_to "Change Password", admin_pass_change_path(), {:method => :get, class: "hvr-underline-from-left  btn btn-lg btn-default"}%> </h2>
   <br></br>
  
   <h2 class="header-link content-link"><%= button_to "Delete Profile", admin_destroy_path(current_admin.id), :method => :get, data: {confirm: "Are you sure you want to delete your profile? This action cannot be undone."},  class: "hvr-underline-from-left  btn btn-lg btn-default"%> </h2>
	</div>
</div>
